{
  "name": "bangsamu/export-runner",
  "description": "Export Excel Runner integration with Laravel job queue using Go binary",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
        "name": "infomediaku.com",
        "email": "hatur.cms@gmail.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Bangsamu\\ExportRunner\\": "src/"
    }
  },
